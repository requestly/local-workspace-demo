{
  "name": "Update a organization",
  "request": {
    "type": "http",
    "url": "{{baseUrl}}/organizations/:id",
    "scripts": {
      "preRequest": "(function () {\n  const updatedName = rq.environment.get(\"ORG_NAME\") + \" Updated\";\n  rq.environment.set(\"ORG_UPDATED_NAME\", updatedName);\n})();\n",
      "postResponse": "(function () {\n  const res = rq.response.json();\n\n  rq.test(\"Org updated successfully\", () => {\n    rq.expect(res.success).to.eql(true);\n  });\n\n  rq.test(\"Name updated\", () => {\n    rq.expect(res.data.name).to.eql(\n      rq.environment.get(\"ORG_UPDATED_NAME\")\n    );\n  });\n})();\n"
    },
    "method": "PATCH",
    "queryParams": [],
    "headers": [
      {
        "isEnabled": true,
        "value": "application/json",
        "key": "Content-Type",
        "id": 0
      },
      {
        "value": "application/json",
        "key": "Accept",
        "isEnabled": true,
        "id": 1
      }
    ],
    "body": "{\n  \"name\": \"{{ORG_UPDATED_NAME}}\"\n}\n",
    "bodyContainer": {
